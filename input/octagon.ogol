
---------------------------------------------------------------------------

to dash :n :len
   repeat :n [ pd; fd :len; pu; fd :len; ]
   bk :len;
   pd;
end

---------------------------------------------------------------------------

to octagon
   repeat 8 [fd 200; squareDashTwirl 8; rt 45;]
end

---------------------------------------------------------------------------

to squareDash :n :len
   repeat 4 [ dash :n :len; rt 90; ]
end

---------------------------------------------------------------------------

to squareDashTwirl :n
   repeat 36 [squareDash :n :n; rt 10;]
end

