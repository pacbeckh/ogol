to dash :n :len
   repeat :n [ pd; fd :len; pu; fd :len; ]
   bk :len;
   hello 50;
   pd ;
end

to squareDash :n :len
   repeat 4 [
   	to bar foo; end
   	to foo bar; end
    dash :n :len; rt 90; 
    ]
end

repeat 60 [repeat 360 [forward 1; left 1;] left 10;]
ifelse true [dash 1 5;][squareDash 4 8;]
dash 10 20;